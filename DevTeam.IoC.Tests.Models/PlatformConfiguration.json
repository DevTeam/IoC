[
    { "reference": "DevTeam.IoC.Tests.Models" },
    { "reference": "DevTeam.IoC.Contracts" },

    { "using": "DevTeam.IoC.Tests.Models" },
    { "using": "DevTeam.IoC.Contracts" },

    {
        "GetDependecies": [
            { "Wellknown": "All" }
        ]
    },

    {
        "Apply": [
            {
                "Register": [
                    { "Lifetime": "Singleton" },
                    { "Contract": [ "ITrace" ] }
                ],
                "AsAutowiring": "Trace"
            },

            {
                "Register": [
                    { "Lifetime": "Singleton" },
                    { "Contract": [ "IConsole" ] }
                ],
                "AsAutowiring": "Console",
                "Constructor": [
                    {
                            
                        "Type": "IProvider<ITrace>",
                        "Dependency": [
                            { "Contract": [ "IProvider<ITrace>" ] }
                        ]
                    }
                ]
            },

            {
                "Register": [
                    { "Lifetime": "Singleton" },
                    { "Contract": [ "ITimer" ] },
                    { "Contract": [ "ITimerManager" ] }
                ],
                "AsAutowiring": "Timer"
            },

            {
                "Register": [
                    { "State": "0", "TypeName": "string"},
                    { "Contract": [ "ILog" ] }
                ],
                "AsAutowiring": "Log"
            }
        ]
    }
]
